{
  "name": "playgamerecruitmenttest",
  "version": "1.0.0",
  "description": "Simple Banking System For Playgame Recruitment Process",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "serve": "node dist/main.js",
    "create-db": "sequelize db:create",
    "migrate-db": "sequelize db:migrate",
    "dev": "nodemon --watch '**/*' -e ts,tsx --exec ts-node src/main.ts",
    "prod": "npm run tslint && npm run build && npm run serve"
  },
  "author": "Nanda Julianda Akbar <nanda.julianda48@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "md5": "^2.2.1",
    "mongoose": "^5.4.4",
    "nodemon": "^1.18.9",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.3.3",
    "sequelize": "^4.42.0",
    "sequelize-typescript": "^0.6.7",
    "tsyringe": "^3.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "sequelize-cli": "^5.4.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  }
}
